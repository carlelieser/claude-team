{
  "name": "@claude-team/cli",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "claude-team": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup src/index.ts --format esm --watch --onSuccess 'node dist/index.js'",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@claude-team/core": "workspace:*",
    "@claude-team/database": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "better-sqlite3": "^11.6.0",
    "ora": "^8.1.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.18.0",
    "vitest": "^2.1.0"
  }
}
